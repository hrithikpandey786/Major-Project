generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model EnrolledStudent{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  enrolmentNo Int @unique
  name String
  dob DateTime @db.Date 
}

model RegisteredStudent{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  enrolmentNo Int @unique
  name String
  dob DateTime @db.Date
  email String @unique
  phoneNumber Int @unique
  department String
  course String
  gender String
  password String
}

model Student{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  enrolmentNo Int @unique
  name String
  fatherName String
  motherName String
  dob DateTime @db.Date
  email String @unique
  phoneNumber Int @unique
  department String
  course String
  gender String
  admissionYear Int
  resultDate DateTime @db.Date
  status String @default("Pending at Department")
  address String
  pincode Int
  type String
  reappearance String
}

model Faculty{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  facultyName String @unique
}
























































































